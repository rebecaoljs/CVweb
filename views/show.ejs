<% include partials/header %>

<!-- =========================================== INFORMAÇÃO PESSOAL ============================================ -->
<div class="container">
  <div class="personalInformation">
    <h3 class="titleDetailPage">INFORMAÇÃO PESSOAL</h3>
    <table class="tableDetailPage">
      <tr>
        <td>
          <h3 id="fullNameDetailPAge">
            <%= foundCV.firstName %> <%= foundCV.lastName %>
          </h3>
        </td>
      </tr>
      <tr>
        <td>
          <%= foundCV.streetNumber %>, <%= foundCV.postcode %> - <%=
          foundCV.cityInfo %> (<%= foundCV.countryInfo %>)
        </td>
      </tr>
      <tr>
        <td><%= foundCV.typephone %>: <%= foundCV.phone %></td>
      </tr>
      <tr>
        <td>
          <a href="<%= foundCV.PersonalWebPage %>"
            ><%= foundCV.PersonalWebPage %></a
          >
        </td>
      </tr>
      <tr>
        <td><%= foundCV.email %></td>
      </tr>
      <tr>
        <td><%= foundCV.typeContactMsg %>: <%= foundCV.msgContact %></td>
      </tr>
      <tr>
        <td>
          Data de Nascimento: <%= foundCV.dayFormBirthday %>/<%=
          foundCV.monthFormBirthday %>/<%= foundCV.yearFormBirthday %>
        </td>
      </tr>
      <tr>
        <td>Nacionalidade: <%= foundCV.nationality %></td>
      </tr>
    </table>

    <!-- <%= foundCV.dateFormatBirthday %>  -->
  </div>
  <!-- =========================================== TIPO DE CANDIDATURA ============================================ -->
  <div class="applicationType">
    <h3 class="titleDetailPage"><%= foundCV.application %></h3>
    <table class="tableDetailPage">
      <tr>
        <td><%= foundCV.applicationDescription %></td>
      </tr>
    </table>
  </div>
  <!-- =========================================== EXPERIÊNCIA PROFISSIONAL ============================================ -->
  <div class="professionalExperience">
    <h3 class="titleDetailPage">EXPERIÊNCIA PROFISSIONAL</h3>
    <table class="tableDetailPage">
      <tr>
        <td><%= foundCV.role %></td>
      </tr>
      <tr>
        <td>
          <%= foundCV.employer %>, <%= foundCV.cityEmployer %> (<%=
          foundCV.countryEmployer %>)
        </td>
      </tr>
      <tr>
        <td>
          Início: <%= foundCV.dateDayExperienceFrom %>/<%=
          foundCV.dateMonthExperienceFrom %>/<%= foundCV.dateYearExperienceFrom
          %> - Fim: <%= foundCV.dateDayExperienceTo %>/<%=
          foundCV.dateMonthExperienceTo %>/<%= foundCV.dateYearExperienceTo %>
        </td>
        <td><%= foundCV.onGoingExperience %></td>
      </tr>
      <!-- <tr>
      <td><%= foundCV.dateFormatExperience %></td>
    </tr> -->
      <!-- <tr>
        <td><%= foundCV.moreDetailExperience %></td>
      </tr> -->
      <!-- <tr>
        <td><%= foundCV.adressEmployer %></td>
