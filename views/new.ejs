<% include partials/header %></include>

<form action="/" method="POST">
    <div class="form-group form-head-center">
        <label for="personalInfo"></label>
        <h2 id="personalInfo">INFORMAÇÃO PESSOAL</h2>
    </div>
    <div class="container" id="firstSectionForm">
        <div class="form-group">
            <label for="firstName">Nome(s) próprio(s):</label>
            <input id="firstName" class="form-control form-control-lg" type="text" name="nome" placeholder="Ex.: Lucas">
        </div>
        <div class="form-group">
            <label for="lastName">Apelido(s):</label>
            <input id="lastName" class="form-control form-control-lg" type="text" name="apelido"
                placeholder="Ex.: Ramos">
        </div>
        <div class="form-group">
            <label for="adressNumber">Nome da rua e número:</label>
            <input id="adressNumber" class="form-control form-control-lg" type="text" name="ruaNumero"
                placeholder="Ex.: Rua/Número">
        </div>
        <div class="form-row">
            <div class="form-group col-md-4">
                <label for="postCode">Código postal:</label>
                <input id="postCode" class="form-control form-control-lg" type="number" name="codigoPostal"
                    placeholder="Ex.: 1749-321">
            </div>
            <div class="form-group col-md-4">
                <label for="city">Cidade:</label>
                <input id="city" class="form-control form-control-lg" type="text" name="cidadeInfo"
                    placeholder="Ex.: Lisboa">
            </div>
            <div class="form-group col-md-4">
                <label for="countryList">País:</label>
                <select id="countryList" class="form-control custom-select-lg custom-select" name="paisInfo">
                    <% country(); %>
                </select>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="tel">Telefone(s):</label>
                <input id="tel" class="form-control form-control-lg" type="tel" name="tel"
                    placeholder="Ex.: (+351) 123 45 67 89">
            </div>
            <div class="form-group col-md-6">
                <label for="type">Tipo(s):</label>
                <select id="type" class="form-control custom-select-lg custom-select" name="tipo">
                    <option value="Telemóvel">Telemóvel</option>
                    <option value="Casa">Casa</option>
                    <option value="Trabalho">Trabalho</option>
                </select>
            </div>
            <!-- <div class="form-group col-md-1" id="background-btn">
                <button class="btn"><i class="far fa-plus-square"></i></button>
            </div> -->
        </div>
        <div class="form-group">
            <label for="webPage">Página Web / Blog / LinkedIn:</label>
            <input id="webPage" class="form-control form-control-lg" type="url" name="webPage"
                placeholder="www.linkedin.com/lucasramos">
        </div>
        <div class="form-group">
            <label for="email">Email:</label>
            <input id="email" class="form-control form-control-lg" type="email" name="email"
                placeholder="Ex.: lucas_ramos@mail.pt">
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="msg">Mensagem Instantânea:</label>
                <input id="msg" class="form-control form-control-lg" type="text" name="msg"
                    placeholder="Ex.: lucas.ramos">
            </div>
            <div class="form-group col-md-6">
                <label for="candidatura">Tipo(s):</label>
                <select id="candidatura" class="form-control custom-select-lg custom-select" name="tipoMsg">
                    <option value="Skype">Skype</option>
                    <option value="Whatsapp">Whatsapp</option>
                    <option value="Google Hangouts">Google Hangouts</option>
                    <option value="ICQ">ICQ</option>
                    <option value="AOL Instant Messeger(AIM)">AOL Instant Messeger(AIM)</option>
                    <option value="MSN">MSN</option>
                    <option value="Yahoo! Messeger (YIM)">Yahoo! Messeger (YIM)</option>
                </select>
            </div>
        </div>
        <div class="custom-control custom-checkbox">
            <input class="custom-control-input" id="maisDetalhePessoal" type="checkbox" onclick="hideDetalhePessoal()">
            <label class="custom-control-label" for="maisDetalhePessoal">Campos extra</label>
        </div>
        <div class="form-group campoExtra" id="campoExtraPessoal">
            <div class="form-group">
                <p> Preencher, caso necessário. Verifique disposições aplicáveis sobre o fornecimento de dados, tais
                    como sexo, idade, nacionalidade, etc. para o CV.</p>
            </div>
            <div>
                <div class="form-group">
                    <p>Sexo:</p>
                </div>
                <div class="form-group form-inline">
                    <div class="custom-control custom-radio radio-space">
                        <input class="custom-control-input" name="radioExtra" id="man" type="radio">
                        <label class="custom-control-label" for="man">Masculino</label>
                    </div>
                    <div class="custom-control custom-radio radio-space">
                        <input class="custom-control-input" name="radioExtra" id="woman" type="radio">
                        <label class="custom-control-label" for="woman">Feminino</label>
                    </div>
                    <div class="custom-control custom-radio radio-space">
                        <input class="custom-control-input" name="radioExtra" id="naoIndicar" type="radio">
                        <label class="custom-control-label" for="naoIndicar">Não indicar</label>
                    </div>
                </div>
            </div>

            <div class="form-row">
                <div class="form-group col-md-4">
                    <label for="dayFormBirthday">Data de nascimento:</label>
                    <select class="form-control custom-select-lg custom-select" name="dayFormBirthday"
                        id="dayFormBirthday">
                        <% day(); %>
                    </select>
                </div>
                <div class="form-group col-md-4">
                    <label for="monthFormBirthday">.</label>
                    <select class="form-control custom-select-lg custom-select" name="monthFormBirthday"
                        id="monthFormBirthday">
                        <% month(); %>
                    </select>
                </div>
                <div class="form-group col-md-4">
                    <label for="yearFormBirthday">.</label>
                    <select class="form-control custom-select-lg custom-select" name="yearFormBirthday"
                        id="yearFormBirthday">
                        <% year(); %>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label for="dateFormat">Formato da data:</label>
                <select id="dateFormat" class="form-control custom-select-lg custom-select" name="formatoData">
                    <option data-value="numeric/long">31/01/2005</option>
                    <option data-value="text/short">31 Jan 05</option>
                    <option data-value="numeric/short">31/1/05</option>
                    <option data-value="numeric/medium">31/1/2005</option>
                </select>
            </div>
            <div class="form-group">
                <label for="nacionalidade">Nacionalidade:</label>
                <select id="nacionalidade" class="form-control custom-select-lg custom-select" name="nacionalidade">
                    <% nationalities(); %>
                </select>
            </div>
        </div>
    </div>

    <!-- ========================== SECTION TWO ========================== -->
    <div class="form-group form-head-center">
        <label for="candidaturaInfo"></label>
        <h2 ID="candidaturaInfo">POSTO DE TRABALHO A QUE SE CANDIDATA</h2>
    </div>
    <div class="container" id="secondSectionForm">
        <div class="form-group">
            <label for="candidatura">Tipo de candidatura:</label>
            <select id="candidatura" class="form-control custom-select-lg custom-select" name="candidatura">
                <option value="POSTO DE TRABALHO A QUE SE CANDIDATA">POSTO DE TRABALHO A QUE SE CANDIDATA</option>
                <option value="PROFISSÃO">PROFISSÃO</option>
                <option value="EMPREGO PRETENDIDO">EMPREGO PRETENDIDO</option>
                <option value="ESTUDOS A QUE SE CANDIDATA">ESTUDOS A QUE SE CANDIDATA</option>
                <option value="DECLARAÇÃO PESSOAL">DECLARAÇÃO PESSOAL</option>
            </select>
        </div>
        <div class="form-group">
            <label for="descricao">Descrição:</label>
            <select name="descricao" class="form-control custom-select-lg custom-select" id="descricao">
                <% job(); %>
            </select>
        </div>
    </div>

    <!-- ========================== SECTION THREE ========================== -->
    <div class="form-group form-head-center">
        <label for="experienceInfo"></label>
        <h2 ID="experienceInfo">EXPERIÊNCIA PRFISSIONAOL</h2>
    </div>
    <div class="container" id="thirdSectionForm">
        <div class="form-row">
            <div class="form-group col-md-4">
                <label for="dateDayExperience-De">De:</label>
                <select class="form-control custom-select-lg custom-select" name="dateDayExperience-De"
                    id="dateDayExperience-De">
                    <% day(); %>
                </select>
            </div>
            <div class="form-group col-md-4">
                <label for="dateMonthExperience-De">.</label>
                <select class="form-control custom-select-lg custom-select" name="dateMonthExperience-De"
                    id="dateMonthExperience-De">
                    <% month(); %>
                </select>
            </div>
            <div class="form-group col-md-4">
                <label for="dateYearExperience-De">.</label>
                <select class="form-control custom-select-lg custom-select" name="dateYearExperience-De"
                    id="dateYearExperience-De">
                    <% year(); %>
                </select>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-4">
                <label for="dateDayExperience-A">A:</label>
                <select class="form-control custom-select-lg custom-select" name="dateDayExperience-A"
                    id="dateDayExperience-A">
                    <% day(); %>
                </select>
            </div>
            <div class="form-group col-md-4">
                <label for="dateMonthExperience-A">.</label>
                <select class="form-control custom-select-lg custom-select" name="dateMonthExperience-A"
                    id="dateMonthExperience-A">
                    <% month(); %>
                </select>
            </div>
            <div class="form-group col-md-4">
                <label for="dateYearExperience-A">.</label>
                <select class="form-control custom-select-lg custom-select" name="dateYearExperience-A"
                    id="dateYearExperience-A">
                    <% year(); %>
                </select>
            </div>
        </div>
        <div class="custom-control custom-checkbox">
            <input class="custom-control-input" id="continuoExperience" type="checkbox">
            <label class="custom-control-label" for="continuoExperience">Contínuo</label>
        </div>
        <div class="form-group">
            <label for="dateFormat">Formato da data:</label>
            <select id="dateFormat" class="form-control custom-select-lg custom-select" name="formatoData">
                <option data-value="numeric/long">31/01/2005</option>
                <option data-value="text/short">31 Jan 05</option>
                <option data-value="numeric/short">31/1/05</option>
                <option data-value="numeric/medium">31/1/2005</option>
            </select>
        </div>
        <div class="form-group">
            <label for="cargo">Função ou cargo ocupado:</label>
            <select name="cargo" class="form-control custom-select-lg custom-select" id="cargo">
                <% job(); %>
            </select>
        </div>
        <h5>EMPREGADOR</h5>
        <div class="form-group">
            <label for="nomeEmpregador">Nome:</label>
            <input id="nomeEmpregador" class="form-control form-control-lg" type="text" name="nomeEmpregador"
                placeholder="Ex.: Microsoft">
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="city">Cidade:</label>
                <input id="city" class="form-control form-control-lg" type="text" name="cidadeInfo"
                    placeholder="Ex.: Lisboa">
            </div>
            <div class="form-group col-md-6">
                <label for="countryList">País:</label>
                <select id="countryList" class="form-control custom-select-lg custom-select" name="paisInfo">
                    <% country(); %>
                </select>
            </div>
        </div>
        <div class="custom-control custom-checkbox">
            <input class="custom-control-input" id="maisDetalheExperience" type="checkbox"
                onclick="hideDetalheExperience()">
            <label class="custom-control-label" for="maisDetalheExperience">Mais Detalhes(complete apenas se
                necessário)</label>
        </div>
        <div class="form-group campoExtra" id="campoExtraExperience">
            <div class="row">
                <div class="form-group col-md-9">
                    <label for="adressNumber">Nome da rua e número:</label>
                    <input id="adressNumber" class="form-control form-control-lg" type="text" name="ruaNumero"
                        placeholder="Ex.: Rua/Número">
                </div>
                <div class="form-group col-md-3">
                    <label for="postCode">Código postal:</label>
                    <input id="postCode" class="form-control form-control-lg" type="number" name="codigoPostal"
                        placeholder="Ex.: 1749-321">
                </div>
            </div>
            <div class="form-group">
                <label for="websiteEmpresa">Website:</label>
                <input id="websiteEmpresa" class="form-control form-control-lg" type="text" name="websiteEmpresa"
                    placeholder="Ex.: https://www.microsoft.com/pt-pt">
            </div>
            <div class="form-group">
                <label for="setor">Tipo de empresa ou setor de atividade:</label>
                <select id="setor" class="form-control custom-select-lg custom-select" name="setor">
                    <% setor(); %>
                </select>
            </div>
        </div>
        <div class="form-group form-block">
            <label for="actividade">Principais actividades e responsabilidades:</label>
            <textarea id="actividade" name="actividade" cols="" rows=5></textarea>
        </div>
    </div>

    <!-- ========================== SECTION FOUR ========================== -->
    <div class="form-group form-head-center">
        <label for="eduInfo"></label>
        <h2 ID="eduInfo">EDUCAÇÃO E FORMAÇÃO</h2>
    </div>
    <div class="container" id="fourSectionForm">
        <div class="form-row">
            <div class="form-group col-md-4">
                <label for="dateDayEdu-De">De:</label>
                <select class="form-control custom-select-lg custom-select" name="dateDayEdu-De" id="dateDayEdu-De">
                    <% day(); %>
                </select>
            </div>
            <div class="form-group col-md-4">
                <label for="dateMonthEdu-De">.</label>
                <select class="form-control custom-select-lg custom-select" name="dateMonthEdu-De" id="dateMonthEdu-De">
                    <% month(); %>
                </select>
            </div>
            <div class="form-group col-md-4">
                <label for="dateYearEdu-De">.</label>
                <select class="form-control custom-select-lg custom-select" name="dateYearEdu-De" id="dateYearEdu-De">
                    <% year(); %>
                </select>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-4">
                <label for="dateDayEdu-A">A:</label>
                <select class="form-control custom-select-lg custom-select" name="dateDayEdu-A" id="dateDayEdu-A">
                    <% day(); %>
                </select>
            </div>
            <div class="form-group col-md-4">
                <label for="dateMonthEdu-A">.</label>
                <select class="form-control custom-select-lg custom-select" name="dateMonthEdu-A" id="dateMonthEdu-A">
                    <% month(); %>
                </select>
            </div>
            <div class="form-group col-md-4">
                <label for="dateYearEdu-A">.</label>
                <select class="form-control custom-select-lg custom-select" name="dateYearEdu-A" id="dateYearEdu-A">
                    <% year(); %>
                </select>
            </div>
        </div>
        <div class="custom-control custom-checkbox">
            <input class="custom-control-input" id="continuoEu" type="checkbox">
            <label class="custom-control-label" for="continuoEu">Contínuo</label>
        </div>
        <div class="form-group">
            <label for="dateFormat">Formato da data:</label>
            <select id="dateFormat" class="form-control custom-select-lg custom-select" name="formatoData">
                <option data-value="numeric/long">31/01/2005</option>
                <option data-value="text/short">31 Jan 05</option>
                <option data-value="numeric/short">31/1/05</option>
                <option data-value="numeric/medium">31/1/2005</option>
            </select>
        </div>
        <div class="form-group">
            <label for="qualification">Designação da qualificação atribuída:</label>
            <input id="qualification" class="form-control form-control-lg" type="text" name="qualification"
                placeholder="Certificado de Técnico em Eletrónica e Automação Naval">
        </div>
        <h5>Organização de ensino ou formação</h5>
        <div class="form-group">
            <label for="ensino">Nome:</label>
            <input id="ensino" class="form-control form-control-lg" type="text" name="ensino"
                placeholder="Escola Superior Náutica Infante D. Henrique">
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="city">Cidade:</label>
                <input id="city" class="form-control form-control-lg" type="text" name="cidadeInfo"
                    placeholder="Ex.: Lisboa">
            </div>
            <div class="form-group col-md-6">
                <label for="countryList">País:</label>
                <select id="countryList" class="form-control custom-select-lg custom-select" name="paisInfo">
                    <% country(); %>
                </select>
            </div>
        </div>
        <div class="custom-control custom-checkbox">
            <input class="custom-control-input" id="maisDetalheEdu" type="checkbox" onclick="hideDetalheEdu()">
            <label class="custom-control-label" for="maisDetalheEdu">Mais Detalhes(complete apenas se
                necessário)</label>
        </div>
        <div class="form-group campoExtra" id="campoExtraEdu">
            <div class="row">
                <div class="form-group col-md-9">
                    <label for="adressNumber">Nome da rua e número:</label>
                    <input id="adressNumber" class="form-control form-control-lg" type="text" name="ruaNumero"
                        placeholder="Ex.: Rua/Número">
                </div>
                <div class="form-group col-md-3">
                    <label for="postCode">Código postal:</label>
                    <input id="postCode" class="form-control form-control-lg" type="number" name="codigoPostal"
                        placeholder="Ex.: 1749-321">
                </div>
            </div>
            <div class="form-group">
                <label for="websiteEdu">Website:</label>
                <input id="websiteEdu" class="form-control form-control-lg" type="text" name="websiteEdu"
                    placeholder="Ex.: https://www.enautica.pt/">
            </div>
        </div>
        <div class="form-group">
            <label for="qeq">Nível QEQ ou classificação nacional:</label>
            <select id="qeq" class="form-control custom-select-lg custom-select" name="qeq">
                <option value="Selecionar">Selecionar</option>
                <option value="">----</option>
                <option value="Nível 1 QRQ">Nível 1 QRQ</option>
                <option value="Nível 2 QRQ">Nível 2 QRQ</option>
                <option value="Nível 3 QRQ">Nível 3 QRQ</option>
                <option value="Nível 4 QRQ">Nível 4 QRQ</option>
                <option value="Nível 5 QRQ">Nível 5 QRQ</option>
                <option value="Nível 6 QRQ">Nível 6 QRQ</option>
                <option value="Nível 7 QRQ">Nível 7 QRQ</option>
                <option value="Nível 8 QRQ">Nível 8 QRQ</option>
            </select>
        </div>
        <div class="form-group form-block">
            <label for="disciplinas">Principais disciplinas / competências profissionais:</label>
            <textarea id="disciplinas" name="disciplinas" cols="" rows="5"></textarea>
        </div>
    </div>

    <!-- ========================== SECTION FIVE ========================== -->
    <div class="container" id="fiveSectionForm">
        <div class="form-group form-head-center">
            <label for="competenciasInfo"></label>
            <h2 ID="competenciasInfo">COMPETÊNCIAS PESSOAIS</h2>
        </div>
        <div class="form-group">
            <label for="motherLanguage">Língua materna:</label>
            <select id="motherLanguage" class="form-control custom-select-lg custom-select" name="motherLanguage">
                <% language(); %>
            </select>
        </div>
        <div class="form-group form-block">
            <label for="comunicacao">Competências de comunicação:</label>
            <textarea id="comunicacao" name="comunicacao" cols="" rows="5"></textarea>
        </div>
        <div class="form-group form-block">
            <label for="organizacao">Competências de organização:</label>
            <textarea id="organizacao" name="organizacao" cols="" rows="5"></textarea>
        </div>
        <div class="form-group form-block">
            <label for="compTrabalho">Competências relacionadas com o trabalho:</label>
            <textarea id="compTrabalho" name="compTrabalho" cols="" rows="5"></textarea>
        </div>
        <div class="form-group">
            <label for="lEstrangeira">Línguas Estrangeiras:</label>
            <select id="lEstrangeira" class="form-control custom-select-lg custom-select" name="lEstrangeira">
                <% language(); %>
            </select>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                <label for="compreensaoOral">Compreender:</label>
                <select id="compreensaoOral" class="form-control custom-select-lg custom-select" name="compreensaoOral">
                    <% speak(); %>
                </select>
            </div>
            <div class="form-group col-md-6">
                <label for="compreensaoLeitura">.</label>
                <select id="compreensaoLeitura" class="form-control custom-select-lg custom-select"
                    name="compreensaoLeitura">
                    <% read(); %>
                </select>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                <label for="interacaoOral">Falar:</label>
                <select id="interacaoOral" class="form-control custom-select-lg custom-select" name="interacaoOral">
                    <% speakfluence(); %>
                </select>
            </div>
            <div class="form-group col-md-6">
                <label for="producaoOral">.</label>
                <select id="producaoOral" class="form-control custom-select-lg custom-select" name="producaoOral">
                    <% speakfluenceLevel(); %>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label for="escrever">Escrever:</label>
            <select id="escrever" class="form-control custom-select-lg custom-select" name="escrever">
                <% write(); %>
            </select>
        </div>
        <div class="form-group">
            <label for="diplomalinguas">Diploma(s) ou certificado(s):</label>
            <input id="diplomalinguas" class="form-control form-control-lg" type="text" name="diplomalinguas"
                placeholder="Ex.: Diploma de Estudos em Língua Espanhola (DELF)...">
        </div>
        <div class="form-group">
            <label for="competenciasDigitais"></label>
            <h2 ID="competenciasDigitais">Competências Digitais:</h2>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                <label for="processamentoInfo">Processamento de informação:</label>
                <select id="processamentoInfo" class="form-control custom-select-lg custom-select"
                    name="processamentoInfo">
                    <% processamento(); %>
                </select>
            </div>
            <div class="form-group col-md-6">
                <label for="comunicacao">Comunicação</label>
                <select id="comunicacao" class="form-control custom-select-lg custom-select" name="comunicacao">
                    <% comunicacao(); %>
                </select>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                <label for="criacaoConteudo">Criação de conteúdos:</label>
                <select id="criacaoConteudo" class="form-control custom-select-lg custom-select" name="criacaoConteudo">
                    <% conteudo(); %>
                </select>
            </div>
            <div class="form-group col-md-6">
                <label for="seguranca">Segurança</label>
                <select id="seguranca" class="form-control custom-select-lg custom-select" name="seguranca">
                    <% seguranca(); %>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label for="resolucaoProblemas">Resolução de problemas:</label>
            <select id="resolucaoProblemas" class="form-control custom-select-lg custom-select"
                name="resolucaoProblemas">
                <% resolucaoProblemas(); %>
            </select>
